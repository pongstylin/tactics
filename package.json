    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
