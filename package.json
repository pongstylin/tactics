{
  "name": "tactics",
  "version": "2.0.3",
  "description": "A tactical turn-based board game",
  "main": "src/tactics.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:pongstylin/tactics.git"
  },
  "scripts": {
    "start": "./start.sh",
    "watch": "NODE_ENV=development webpack --watch --config webpack.config.cjs",
    "compile": "tsc",
    "lint": "tslint",
    "build": "webpack --config webpack.config.cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "NODE_ENV=production webpack --config webpack.config.cjs",
    "clean": "rm -f src/data/ddb/shared-local-instance.db && rm -rf src/models/*.js && rm -f tsconfig.tsbuildinfo",
    "dinstall": "docker compose exec app npm install",
    "script": "docker compose exec app node --require dotenv/config",
    "memscript": "node --optimize-for-size --max-old-space-size=4096 --require dotenv/config"
  },
  "imports": {
    "#package.json": "./package.json",
    "#config/*": "./config/*",
    "#server/*": "./src/server/*",
    "#models/*": "./src/models/*",
    "#data/*": "./src/data/*",
    "#tactics/*": "./src/tactics/*",
    "#utils/*": "./src/utils/*",
    "#plugins/*": "./src/plugins/*"
  },
  "author": "Dave Clark",
  "license": "Unlicense",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@types/uuid": "^8.3.1",
    "babel-loader": "^8.2.2",
    "core-js": "^3.21.1",
    "css-loader": "^5.2.7",
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^2.0.0",
    "typescript": "^5.9.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.751.0",
    "@aws-sdk/lib-dynamodb": "^3.751.0",
    "@metrichor/jmespath": "^0.3.1",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "bufferutil": "^4.0.1",
    "decancer": "^1.6.5",
    "discord.js": "^14.14.1",
    "dynamodb-size": "^1.0.9",
    "events": "^3.3.0",
    "express": "^4.16.4",
    "howler": "^2.2.4",
    "impetus": "^0.8.8",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.9.1",
    "nosleep.js": "github:pongstylin/nosleep.js",
    "obscenity": "^0.2.0",
    "openid-client": "^5.6.2",
    "pixi.js": "^8.7.2",
    "seedrandom": "^3.0.5",
    "string-pixel-width": "^1.10.0",
    "touch-pinch": "^1.0.1",
    "ua-parser-js": "^1.0.37",
    "utf-8-validate": "^5.0.2",
    "uuid": "^9.0.1",
    "web-push": "^3.6.6",
    "workerpool": "^9.2.0",
    "ws": "^8.16.0",
    "xregexp": "^5.1.1"
  }
}
