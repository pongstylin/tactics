{
  "name": "tactics",
  "version": "0.15.1",
  "description": "A tactical turn-based board game",
  "main": "src/tactics.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:pongstylin/tactics.git"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack --watch --config webpack.config.cjs & node --es-module-specifier-resolution=node --experimental-modules --experimental-loader ./resolver.mjs --require dotenv/config src/server.js || true",
    "start-beta": "NODE_ENV=beta node --experimental-modules --loader ./resolver.mjs --require dotenv/config src/server.js || true",
    "build": "NODE_ENV=beta webpack --config webpack.config.cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "NODE_ENV=production webpack --config webpack.config.cjs",
    "clean": "rm -rf src/data/files && git checkout src/data/files",
    "script": "node --es-module-specifier-resolution=node --experimental-modules --experimental-loader ./resolver.mjs --require dotenv/config"
  },
  "author": "Dave Clark",
  "license": "Unlicense",
